<% if @courses.any? %>
  <% @courses.each do |course| %>
    <div id="course-<%= course.id %>"> 
      <h3><%= course.title %></h3>  
      <p><%= course.description %></p>
      <% if course.deliveries %>
        <p>Medlemsdatum:
          <% course.deliveries.each do |date| %>
           <%= link_to date.start_date, "/courses/deliveries/show/#{date.id}" %> <BR>
          <% end %>
        </p>
      <% end %>
      <%= link_to 'LÃ¤gg till medlemsdatum', "/courses/#{course.id}/add_date" if current_user %>
    </div>
  <% end %>
<% else %>
  <h1>Inga medlemstyper skapade</h1>
<% end %>
<% if current_user %>
<p>
  <%= link_to 'Skapa medlemstyp', '/courses/create' %>
  </p>
<% end %>
